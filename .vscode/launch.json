  "version": "0.2.0",
  "configurations": [
    {
      "console": "integratedTerminal",
      "type": "node",
      "request": "launch",
      "name": "Launch Create from NPM",
      "program": "${workspaceFolder}/dist/create-object-from-npm-package.js",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Import from FTB",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/import-mods-from-ftb.js",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
